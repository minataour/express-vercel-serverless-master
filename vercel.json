{
    "version": 2,
    "builds": [
      {
        "src": "app.js",
        "use": "@now/node"
      },
      {
        "src": "/routes/books.js",
        "use": "@now/node"
      },
      {
        "src": "/routes/book.js",
        "use": "@now/node"
      }
    ],
    "rewrites": [
      {
        "source": "/",
        "destination": "app.js"
      },
      {
        "source": "/books",
        "destination": "/routes/books.js"
      },
      {
        "source": "/book/:id",
        "destination": "api/book"
      }
    ]
  }