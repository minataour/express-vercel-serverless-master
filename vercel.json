{
    "version": 2,
    "builds": [
      {
        "src": "app.js",
        "use": "@now/node"
      }
    ],
    "routes": [
      {
        "src": "/",
        "dest": "app.js"
      },
      {
        "src": "/books",
        "dest": "api/books.js"
      },
      {
        "src": "/book",
        "dest": "/api/book.js"
      }
    ]
  }